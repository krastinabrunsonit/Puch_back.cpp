#include<iostream>
#include<string>
using namespace std;

int main(){
  string secretID = "Barry";
  string lastName = "Allen";
  char spaceChar = '';
  
  //this is practice to retype code to correct
  
  secretID.push_back(spaceChar);
  secretID = secretID + lastName;
  
  cout << secretID << endl;
  return 0;
  }
